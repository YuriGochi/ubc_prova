@model List<ubc.Models.Musica>

<h1>Lista de Músicas</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Título</th> 
            <th>Lugares Tocados</th>
            <th>Lugares</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var musica in Model)
        {
            <tr>
                <td>@musica.Id</td>
                <td>@musica.Titulo</td>
                <td>@musica.lugaresTocados</td>
                <td>@musica.Lugares</td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="Create" class="btn btn-primary">Adicionar Música</a>
